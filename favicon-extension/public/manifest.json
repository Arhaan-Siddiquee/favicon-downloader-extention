{
  "manifest_version": 3,
  "name": "Favicon Downloader",
  "version": "1.0",
  "description": "Downloads favicon of current tab",
  "permissions": ["activeTab", "downloads", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  }
}
